/* pages/my-about/my-about.wxss */
.container {
  display: flex;
  flex-direction: column;
}
.desc-logo {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 60rpx 0;
}
.logo-img {
  width: 250rpx;
  height: 250rpx;
  border-radius: 20rpx;
}
.info {
  margin-top: 20rpx;
}
.footer {
  height: 120rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}