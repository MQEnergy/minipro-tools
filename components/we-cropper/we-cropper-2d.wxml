<template name="we-cropper">
  <canvas
    class="cropper"
    disable-scroll="true"
    bindtouchstart="touchStart"
    bindtouchmove="touchMove"
    bindtouchend="touchEnd"
    style="width:{{width}}px;height:{{height}}px; background: #fff;margin: 0 auto;"
    type="2d"
    id="{{id}}">
  </canvas>
</template>
